<ul>
  <%= @record_label.name %>:
  <ul>
    <% for record in @records %>
  </ul>
  <p>-<%= record.name %>, with a stock quantity of <%= record.quantity %></p>
  <% end %>

  <form action="/record_labels/<%= @record_label.id %>/delete" method="post">
    <input type="submit" value="Delete"/>
  </form>

</ul>
